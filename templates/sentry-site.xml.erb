<?xml version="1.0" encoding="UTF-8"?>
<configuration>

<% props = scope.lookupvar('sentry::_properties') -%>
<%# sort the properties list for ruby <= 1.8 #-%>
<% rv=RUBY_VERSION.split('.') -%>
<% if rv[0] <= '1' and rv[1] <= '8' -%>
<% props_keys = props.keys.sort -%>
<% else -%>
<% props_keys = props.keys -%>
<% end -%>
<% props_keys.each do |p| -%>
<% value = props[p] -%>
<% if value != '::undef' -%>
<% if scope.lookupvar('sentry::descriptions')[p] -%>
  <!-- <%= scope.lookupvar('sentry::descriptions')[p] %> -->
<% end -%>
  <property>
    <name><%= p -%></name>
    <value><%= value -%></value>
  </property>
<% end -%>
<% end -%>

</configuration>
